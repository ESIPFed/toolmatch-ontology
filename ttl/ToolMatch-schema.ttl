                          owl:someValuesFrom :Hovmoller
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :supportsVisualizationType ;
                          owl:someValuesFrom :Map
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :supportsVisualizationType ;
                          owl:someValuesFrom :TimeSeries
                        ] .



