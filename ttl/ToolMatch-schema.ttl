                          owl:someValuesFrom :HDF
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasInputFormat ;
                          owl:someValuesFrom :NetCDF
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :supportsDataType ;
                          owl:someValuesFrom :Grid
                        ] .



###  http://toolmatch.esipfed.org/schema#FTP

:FTP rdf:type owl:Class ;
     
