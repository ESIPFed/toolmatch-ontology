@base <http://toolmatch.esipfed.org/schema> .

<http://toolmatch.esipfed.org/schema> rdf:type owl:Ontology ;
                                      
                                      owl:versionIRI <http://toolmatch.esipfed.org/schema/2.0> ;
                                      
                                      owl:imports time: ,
                                                  foaf: ;
                                      
                                      rdfs:comment "ToolMatch is an ontology and set of rules that can help researchers determine what tools can be used given a particular set of data, or find data that can be used within a tool."@en ;
                                      
                                      dc:rights "This ontology is distributed under a Creative Commons Attribution License http://creativecommons.org/licenses/by/3.0/"@en ;
                                      
                                      dc:publisher twi:ESIPFederation ;
                                      
                                      dc:contributor twi:EricRozell ,
                                                     twi:person/MatthewFerritto ;
                                      
                                      dc:date "2015-11-27"^^xsd:dateTime ;
                                      
                                      owl:versionInfo """v 2.0 
Revision: 9878
Date: 2015-11-26"""@en ;
                                      
                                      dc:contributor twi:person/NancyHoebelheinrich ,
                                                     twi:PatrickWest ;
                                      
                                      rdfs:label "ToolMatch Ontology and Rules"@en ;
                                      
                                      dc:contributor twi:Christopher_Lynnes .


#################################################################
#
#    Annotation properties
#
#################################################################


###  http://purl.org/dc/elements/1.1/contributor

dc:contributor rdf:type owl:AnnotationProperty .



###  http://purl.org/dc/elements/1.1/publisher

dc:publisher rdf:type owl:AnnotationProperty .



###  http://purl.org/dc/elements/1.1/rights

dc:rights rdf:type owl:AnnotationProperty .



###  http://purl.org/dc/terms/description

terms:description rdf:type owl:AnnotationProperty .



###  http://toolmatch.esipfed.org/schema#accessLink

:accessLink rdf:type owl:AnnotationProperty ;
            
            :accessLink "http://upwell.pfeg.noaa.gov/erddap/index.html"^^xsd:anyURI ;
            
            rdfs:subPropertyOf terms:description .



###  http://xmlns.com/foaf/0.1/name

foaf:name rdf:type owl:AnnotationProperty .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://toolmatch.esipfed.org/schema#AccessURL

:AccessURL rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           
           rdfs:domain :DataCollection ;
           
           rdfs:range :URL .



###  http://toolmatch.esipfed.org/schema#canUseAccessProtocol

:canUseAccessProtocol rdf:type owl:ObjectProperty ;
                      
                      rdfs:domain [ rdf:type owl:Restriction ;
                                    owl:onProperty :canUseAccessProtocol ;
                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :DataServer
                                  ] ;
                      
                      rdfs:range :DataAccessProtocol ;
                      
                      dc:description "Specifies the access protocols that the tool can use to access data."@en ;
                      
                      rdfs:label "Can Use Data Access Protocol"@en .



###  http://toolmatch.esipfed.org/schema#canUseDataServer

:canUseDataServer rdf:type owl:ObjectProperty ;
                  
                  rdfs:domain :Tool ;
                  
                  rdfs:range :DataServer ;
                  
                  dc:description "Specifies the type of data servers with which the tool can interract."@en ;
                  
                  rdfs:label "Can Use Data Server"@en .



###  http://toolmatch.esipfed.org/schema#hasCapability

:hasCapability rdf:type owl:ObjectProperty ;
               
               rdfs:domain :Tool ;
               
               rdfs:range :VisualizationType ;
               
               dc:description "Specifies the different facilities available through the tool for visualizing data products."@en ;
               
               rdfs:label "Has Capability"@en .



###  http://toolmatch.esipfed.org/schema#hasDIF

:hasDIF rdf:type owl:ObjectProperty ;
        
        rdfs:comment "GCMD DIF"@en ;
        
        rdfs:label "Has DIF"@en .



###  http://toolmatch.esipfed.org/schema#hasDataConvention

:hasDataConvention rdf:type owl:ObjectProperty ;
                   
                   rdfs:domain :DataFormat ;
                   
                   rdfs:range :DataConvention .



###  http://toolmatch.esipfed.org/schema#hasDataDimension

:hasDataDimension rdf:type owl:ObjectProperty .



###  http://toolmatch.esipfed.org/schema#hasDataFormat

:hasDataFormat rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               
               rdfs:domain :DataCollection ;
               
               rdfs:range :DataFormat ;
               
               dc:description "A Data Collection is stored in a particular data format.   Examples include: hdf5, netcdf, binary, etc."@en ;
               
               rdfs:label "Has Data Format"@en .



###  http://toolmatch.esipfed.org/schema#hasIdentifier

:hasIdentifier rdf:type owl:ObjectProperty ;
               
               rdfs:comment "Data Collection Identifier" ;
               
               rdfs:label "Has Identifier"@en .



###  http://toolmatch.esipfed.org/schema#hasInputFormat

:hasInputFormat rdf:type owl:ObjectProperty ;
                
                rdfs:domain :Tool ;
                
                rdfs:range :DataFormat ;
                
                dc:description "A tool can input or import data products that are encoded in a specified format."@en ;
                
                rdfs:label "Has Input Format"@en .



###  http://toolmatch.esipfed.org/schema#hasOutputFormat

:hasOutputFormat rdf:type owl:ObjectProperty ;
                 
                 rdfs:domain :Tool ;
                 
                 rdfs:range :DataFormat ;
                 
                 dc:description "A tool can output data products in a particular format.  Examples include:  jpg, json, netcdf4, etc."@en ;
                 
                 rdfs:label "Has Output Format"@en .



###  http://toolmatch.esipfed.org/schema#isAccessedBy

:isAccessedBy rdf:type owl:ObjectProperty ;
              
              rdfs:domain :DataCollection ;
              
              rdfs:range :DataServer ;
              
              dc:description "A Data Collection can be accessed by a particular Server."@en ;
              
              rdfs:label "Is Accessed By"@en .



###  http://toolmatch.esipfed.org/schema#isOfType

:isOfType rdf:type owl:ObjectProperty ;
          
          rdfs:domain :Tool ;
          
          rdfs:range :ToolEnvironment ;
          
          dc:description "The tool can be a desktop app, browser app, web service, etc..."@en ;
          
          rdfs:label "Is Of Type"@en .



###  http://toolmatch.esipfed.org/schema#primaryDataConvention

:primaryDataConvention rdf:type owl:ObjectProperty .



###  http://toolmatch.esipfed.org/schema#providesAccess

:providesAccess rdf:type owl:ObjectProperty ;
                
                rdfs:domain :DataServer ;
                
                rdfs:range :DataAccessProtocol ;
                
                dc:description "A Data Server can provide access to data and information using a particular protocol."@en ;
                
                rdfs:label "Provides Access"@en .



###  http://toolmatch.esipfed.org/schema#supportsDataType

:supportsDataType rdf:type owl:ObjectProperty ;
                  
                  rdfs:label "Supports Data Type"@en .



###  http://toolmatch.esipfed.org/schema#supportsVisualizationType

:supportsVisualizationType rdf:type owl:ObjectProperty ;
                           
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           
                           rdfs:domain [ rdf:type owl:Restriction ;
                                         owl:onProperty :supportsVisualizationType ;
                                         owl:someValuesFrom :Tool
                                       ] ;
                           
                           rdfs:range [ rdf:type owl:Restriction ;
                                        owl:onProperty :supportsVisualizationType ;
                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass :VisualizationType
                                      ] ;
                           
                           rdfs:label "Supports Visualization"@en .



###  http://toolmatch.esipfed.org/schema#usesConvention

:usesConvention rdf:type owl:ObjectProperty ;
                
                rdfs:domain :DataCollection ;
                
                rdfs:range :DataConvention ;
                
                dc:description "A data collection follows a particular set of agreed upon rules in its representation and metadata."@en ;
                
                rdfs:label "Uses Convention"@en .



###  http://toolmatch.esipfed.org/schema#visualizedAs2DGRID

:visualizedAs2DGRID rdf:type owl:ObjectProperty .



###  http://toolmatch.esipfed.org/schema#visualizedAs3DGRID

:visualizedAs3DGRID rdf:type owl:ObjectProperty .



###  http://toolmatch.esipfed.org/schema#visualizedBy

:visualizedBy rdf:type owl:ObjectProperty ;
              
              rdfs:domain :DataCollection ;
              
              rdfs:range :Tool ;
              
              dc:description "A Data Collection can be visuzlied by a particular tool."@en ;
              
              rdfs:label "Visualized By"@en .





#################################################################
#
#    Data properties
#
#################################################################


###  http://toolmatch.esipfed.org/schema#AccessURL

:AccessURL rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty .



###  http://toolmatch.esipfed.org/schema#VersionNumber

:VersionNumber rdf:type owl:DatatypeProperty ;
               
               rdfs:label "has Version Number"@en .





#################################################################
#
#    Classes
#
#################################################################


###  http://toolmatch.esipfed.org/schema#AnimatedMap

:AnimatedMap rdf:type owl:Class ;
             
             rdfs:subClassOf :VisualizationType ,
                             [ owl:intersectionOf ( :Latitude
                                                    :Longitude
                                                    :TemporalValue
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
             
             rdfs:label "Animated Map"@en .



###  http://toolmatch.esipfed.org/schema#ArcGIS

:ArcGIS rdf:type owl:Class ;
        
        rdfs:subClassOf :Tool .



###  http://toolmatch.esipfed.org/schema#ArcGISDesktop

:ArcGISDesktop rdf:type owl:Class ;
               
               rdfs:subClassOf :ArcGIS ,
                               :DesktopEnvironment ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasCapability ;
                                 owl:someValuesFrom :Mosaic
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasCapability ;
                                 owl:someValuesFrom :Stitch
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasCapability ;
                                 owl:someValuesFrom :SubsetData
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :supportsDataType ;
                                 owl:someValuesFrom :Grid
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :supportsDataType ;
                                 owl:someValuesFrom :Raster
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :supportsDataType ;
                                 owl:someValuesFrom :Vector
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :supportsVisualizationType ;
                                 owl:someValuesFrom :AnimatedMap
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :supportsVisualizationType ;
                                 owl:someValuesFrom :Histogram
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :supportsVisualizationType ;
                                 owl:someValuesFrom :Map
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :supportsVisualizationType ;
                                 owl:someValuesFrom :ScatterPlot
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :supportsVisualizationType ;
                                 owl:someValuesFrom :TimeSeries
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :supportsVisualizationType ;
                                 owl:someValuesFrom :VerticalProfile
                               ] ;
               
               rdfs:label "ArcGIS Desktop"@en .



###  http://toolmatch.esipfed.org/schema#ArcGISServer

:ArcGISServer rdf:type owl:Class ;
              
              rdfs:subClassOf :ArcGIS ,
                              :DataServer ,
                              :WebBasedEnvironment ;
              
              rdfs:label "ArcGIS Server"@en .



###  http://toolmatch.esipfed.org/schema#ArcGISonline

:ArcGISonline rdf:type owl:Class ;
              
              rdfs:subClassOf :ArcGIS ,
                              :WebBasedEnvironment ;
              
              rdfs:label "ArcGIS Online"@en .



###  http://toolmatch.esipfed.org/schema#BEAT

:BEAT rdf:type owl:Class ;
      
      rdfs:subClassOf :DesktopEnvironment ,
                      :Tool ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasCapability ;
                        owl:someValuesFrom :SubsetData
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :supportsDataType ;
                        owl:someValuesFrom :Grid
                      ] .



###  http://toolmatch.esipfed.org/schema#CDAT

:CDAT rdf:type owl:Class ;
      
      rdfs:subClassOf :Tool ;
      
      rdfs:comment "Climate Data Analysis Tools"@en .



###  http://toolmatch.esipfed.org/schema#CDM_File_Type

:CDM_File_Type rdf:type owl:Class ;
               
               rdfs:subClassOf :DataFormat .



###  http://toolmatch.esipfed.org/schema#Capability

:Capability rdf:type owl:Class .



###  http://toolmatch.esipfed.org/schema#DAP

:DAP rdf:type owl:Class ;
     
     rdfs:subClassOf :DataAccessProtocol .



###  http://toolmatch.esipfed.org/schema#DChart

:DChart rdf:type owl:Class ;
        
        rdfs:subClassOf :Tool .



###  http://toolmatch.esipfed.org/schema#DODS

:DODS rdf:type owl:Class ;
      
      rdfs:subClassOf :DataService ;
      
      rdfs:comment "Data Object Description Service"@en .



###  http://toolmatch.esipfed.org/schema#DOI

:DOI rdf:type owl:Class ;
     
     rdfs:subClassOf :Identifier ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :AccessURL ;
                       owl:someValuesFrom :URL
                     ] .



###  http://toolmatch.esipfed.org/schema#DataAccessProtocol

:DataAccessProtocol rdf:type owl:Class ;
                    
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasInputFormat ;
                                      owl:someValuesFrom :DataFormat
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasOutputFormat ;
                                      owl:someValuesFrom :DataFormat
                                    ] ;
                    
                    dc:description "A standard set of regulations and requirements governing the access of data electronically"@en ;
                    
                    rdfs:label "Data Access Protocol"@en .



###  http://toolmatch.esipfed.org/schema#DataCollection

:DataCollection rdf:type owl:Class ;
                
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasIdentifier ;
                                  owl:someValuesFrom :Identifier
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :isAccessedBy ;
                                  owl:someValuesFrom :DataAccessProtocol
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :usesConvention ;
                                  owl:someValuesFrom :DataConvention
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :visualizedBy ;
                                  owl:someValuesFrom :Tool
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasDataFormat ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :DataFormat
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :AccessURL ;
                                  owl:someValuesFrom xsd:anyURI
                                ] ;
                
                dc:description "A collection of related sets of information that is composed of separate elements but can be manipulated as a unit by a computer."@en ;
                
                rdfs:comment "In some communities this is equivalent to a `Dataset'"@en ;
                
                rdfs:label "Data Collection"@en .



###  http://toolmatch.esipfed.org/schema#DataConvention

:DataConvention rdf:type owl:Class ;
                
                dc:description "A Data Convention is an established technique or practice related to the representation of data in data collections."@en ;
                
                rdfs:label "Data Convention"@en .



###  http://toolmatch.esipfed.org/schema#DataDimension

:DataDimension rdf:type owl:Class .



###  http://toolmatch.esipfed.org/schema#DataFormat

:DataFormat rdf:type owl:Class ;
            
            dc:description "The organization of information according to preset specifications for the storage of data."@en ;
            
            rdfs:label "Data Format"@en .



###  http://toolmatch.esipfed.org/schema#DataOrientation

:DataOrientation rdf:type owl:Class ;
                 
                 rdfs:subClassOf :DataDimension .



###  http://toolmatch.esipfed.org/schema#DataResolution

:DataResolution rdf:type owl:Class ;
                
                rdfs:subClassOf :DataDimension .



###  http://toolmatch.esipfed.org/schema#DataServer

:DataServer rdf:type owl:Class ;
            
            rdfs:subClassOf :ToolEnvironment ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :canUseAccessProtocol ;
                              owl:someValuesFrom :DataAccessProtocol
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasCapability ;
                              owl:someValuesFrom :ServerCapability
                            ] ;
            
            dc:description "A ToolMatch Data Server is a piece of software that can access, manipulate, and return data products derived from datasets, including the datasets themselves"@en ;
            
            rdfs:label "Data Server"@en .



###  http://toolmatch.esipfed.org/schema#DataService

:DataService rdf:type owl:Class ;
             
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :canUseAccessProtocol ;
                               owl:someValuesFrom :DataAccessProtocol
                             ] ;
             
             rdfs:label "Data Service"@en .



###  http://toolmatch.esipfed.org/schema#DataStructure

:DataStructure rdf:type owl:Class ;
               
               rdfs:comment "was 'Data Type'"@en .



###  http://toolmatch.esipfed.org/schema#DesktopEnvironment

:DesktopEnvironment rdf:type owl:Class ;
                    
                    rdfs:subClassOf :ToolEnvironment ;
                    
                    rdfs:label "Desktop Environment"@en .



###  http://toolmatch.esipfed.org/schema#ERDDAP

:ERDDAP rdf:type owl:Class ;
        
        rdfs:subClassOf :DataServer ,
